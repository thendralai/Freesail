{
  "name": "freesail-dev",
  "version": "0.4.2-alpha",
  "private": true,
  "description": "A2UI based Generative UI SDK",
  "workspaces": [
    "packages/@freesail/*",
    "packages/freesail",
    "examples/*"
  ],
  "scripts": {
    "verify": "tsx scripts/verify-catalogs.ts",
    "sync": "node scripts/sync-versions.js",
    "prebuild": "npm run verify && npm run sync",
    "build": "npm run build --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present"
  },
  "keywords": [
    "ai",
    "generative-ui",
    "mcp",
    "a2ui",
    "agent",
    "llm"
  ],
  "author": "Thendral AI Ltd",
  "contributors": [
    "Shanmugam Sudalaimuthu"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "glob": "^13.0.3",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}