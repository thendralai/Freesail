{
  "name": "@freesail/core",
  "version": "0.4.2-alpha",
  "description": "Core TypeScript logic for Freesail - protocol definitions, parser, transport",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "import": "./dist/protocol.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "freesail",
    "a2ux",
    "protocol",
    "generative-ui"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "vite": "7.3.1"
  }
}
