{
  "name": "@freesail/logger",
  "version": "0.4.3-alpha",
  "description": "Standard logging library for Freesail packages",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
