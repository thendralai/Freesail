    "vite": "7.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^4.0.18"
  },
  "author": "Thendral AI Ltd",
  "contributors": [
    "Shanmugam Sudalaimuthu"
