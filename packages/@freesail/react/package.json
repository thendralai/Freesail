{
  "name": "@freesail/react",
  "version": "0.4.3-alpha",
  "description": "React implementation for Freesail - Generative UI SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
