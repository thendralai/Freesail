{
  "name": "@freesail/react",
  "version": "0.4.0-alpha.1",
  "description": "React implementation for Freesail - Generative UI SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
