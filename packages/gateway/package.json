{
  "name": "@freesail/gateway",
  "version": "0.4.0-alpha.1",
  "description": "Freesail Gateway - bridges AI Agents with frontend clients via MCP",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "freesail-gateway": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "freesail",
    "mcp",
    "model-context-protocol",
    "ai-agent",
    "generative-ui"
  ],
  "license": "MIT",
  "dependencies": {
    "@freesail/core": "*",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "express": "^4.18.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "tsx": "^4.7.0",
    "vitest": "^1.0.0"
  }
}
