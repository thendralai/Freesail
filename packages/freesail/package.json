{
  "name": "freesail",
  "version": "0.4.2-alpha",
  "description": "Unified entry point for Freesail AI SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
